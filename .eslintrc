{
  "rules": {
    "indent": [1, 2, {
      "SwitchCase": 1
    }],
    "linebreak-style": [2, "unix"],
    "semi": [1, "always"],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "plugins": [
    "react"
  ]
}
